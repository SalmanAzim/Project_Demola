    <div class="row">
        <div class="container-fluid head">
            <div class="col-lg-5 col-md-5 col-sm-5 col-xs-6">
                <Header class="header mt-top-15 mt-left-25">
                    Salmiakki Forever
                </Header>
            </div>
            <div class="col-lg-7 col-md-7 col-sm-7 col-xs-6 text-right">
                <div class="row mt-top-25">
                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                        <div class="pn white-panel">
                            <ds-widget-clock show-digital digital-format="'dd-MMM-yyyy hh:mm:ss a'"></ds-widget-clock>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row ">
        <div class="container-fluid cust-body">
            <div>
                <circular options="options"></circular>
            </div>
        </div>
