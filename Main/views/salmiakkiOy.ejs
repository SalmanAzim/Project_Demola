                                            <div style="clear: both;">
                                            </div>
                                            <div style="width: 100%; height: 700px;" ag-grid="gridOptions" class="ag-fresh ag-basic">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
